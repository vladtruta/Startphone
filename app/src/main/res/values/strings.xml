    <string name="welcome">Welcome</string>
    <string name="welcome_description">Welcome to Startphone!\n\nFirst, we need to get some things set up. You can try to do it yourself, but if you run into trouble, make sure you have someone next to you.</string>
    <string name="press_get_started_when_ready">Press \'GET STARTED\' when ready</string>
